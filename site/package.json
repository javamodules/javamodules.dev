{
  "name": "@javamodules/site",
  "version": "1.0.0",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "remix vite:build",
    "build:prod": "NODE_ENV=production remix vite:build --mode=production",
    "clean": "rm -fr dist build",
    "dist": "cd .. && pnpm run libs:build && cd site && pnpm run build:prod && mkdir -p dist/assets && cp -frv build/client/* dist/ && cp -frv build/server/* dist/ && cp -frv public/* dist/ && cp -frv build dist/",
    "deploy": "bun run build:prod && pnpm dlx wrangler@latest pages deploy ./build/client",
    "dev": "remix vite:dev",
    "test": "jest --collect-coverage",
    "test:snapshot": "jest --collect-coverage --update-snapshot",
    "lint": "eslint --ignore-path .gitignore --cache --cache-location ./node_modules/.cache/eslint .",
    "start": "pnpm dlx wrangler@latest pages dev ./build/client",
    "typecheck": "tsc",
    "typegen": "pnpm dlx wrangler@latest types",
    "preview": "bun run build && pnpm dlx wrangler@latest pages dev ./build/client",
    "build-cf-types": "pnpm dlx wrangler@latest types",
    "vite:build": "remix vite:build",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "chromatic": "pnpm dlx chromatic@latest --project-token=$CHROMATIC_TOKEN --exit-zero-on-changes --exit-once-uploaded"
  },
  "dependencies": {
    "@mdx-js/react": "3.0.1",
    "@radix-ui/react-slot": "1.0.2",
    "@remix-run/cloudflare": "2.8.1",
    "@remix-run/cloudflare-pages": "2.8.1",
    "@remix-run/react": "2.8.1",
    "class-variance-authority": "0.7.0",
    "clsx": "2.1.0",
    "isbot": "5.1.1",
    "lucide-react": "0.358.0",
    "miniflare": "3.20240304.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "tailwind-merge": "2.2.2",
    "tailwindcss-animate": "1.0.7"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^1.2.22",
    "@cloudflare/workers-types": "4.20240314.0",
    "@codecov/vite-plugin": "0.0.1-beta.4",
    "@jest/globals": "29.7.0",
    "@mdx-js/rollup": "3.0.1",
    "@remix-run/dev": "2.8.1",
    "@storybook/addon-essentials": "8.0.0",
    "@storybook/addon-interactions": "8.0.0",
    "@storybook/addon-links": "8.0.0",
    "@storybook/addon-onboarding": "8.0.0",
    "@storybook/blocks": "8.0.0",
    "@storybook/react": "8.0.0",
    "@storybook/react-vite": "8.0.0",
    "@storybook/test": "8.0.0",
    "@types/mdx": "2.0.11",
    "@types/react": "18.2.66",
    "@types/react-dom": "18.2.22",
    "@typescript-eslint/eslint-plugin": "7.2.0",
    "@typescript-eslint/parser": "7.2.0",
    "@vitejs/plugin-react": "4.2.1",
    "autoprefixer": "10.4.18",
    "chromatic": "11.0.8",
    "eslint": "8.57.0",
    "eslint-import-resolver-typescript": "3.6.1",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-jsx-a11y": "6.8.0",
    "eslint-plugin-react": "7.34.1",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-storybook": "0.8.0",
    "jest": "29.7.0",
    "jest-junit": "16.0.0",
    "node-fetch": "3.3.2",
    "postcss": "8.4.35",
    "react-test-renderer": "18.2.0",
    "remix-ssg": "0.0.8",
    "rollup-plugin-visualizer": "5.12.0",
    "serve": "14.2.1",
    "storybook": "8.0.1",
    "tailwindcss": "3.4.1",
    "ts-jest": "29.1.2",
    "typescript": "5.4.2",
    "vite": "5.1.6",
    "vite-tsconfig-paths": "4.3.2",
    "wrangler": "3.34.2"
  },
  "engines": {
    "node": ">=20.0.0",
    "bun": "1.0.31"
  }
}
