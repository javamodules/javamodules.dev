{
  "name": "@javamodules/site",
  "version": "1.0.0",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "remix vite:build",
    "build:prod": "NODE_ENV=production remix vite:build --mode=production",
    "clean": "rm -fr dist build",
    "dist": "pnpm run build:prod && mkdir -p dist/assets && cp -frv build/client/* dist/ && cp -frv build/server/* dist/ && cp -frv public/* dist/",
    "deploy": "bun run build:prod && wrangler pages deploy ./build/client",
    "dev": "remix vite:dev",
    "lint": "eslint --ignore-path .gitignore --cache --cache-location ./node_modules/.cache/eslint .",
    "start": "wrangler pages dev ./build/client",
    "typecheck": "tsc",
    "typegen": "wrangler types",
    "preview": "bun run build && wrangler pages dev ./build/client",
    "build-cf-types": "wrangler types",
    "vite:build": "remix vite:build"
  },
  "dependencies": {
    "@mdx-js/react": "^3.0.1",
    "@radix-ui/react-slot": "^1.0.2",
    "@remix-run/cloudflare": "^2.8.0",
    "@remix-run/cloudflare-pages": "^2.8.0",
    "@remix-run/react": "^2.8.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "isbot": "^4.1.0",
    "lucide-react": "^0.358.0",
    "miniflare": "^3.20231030.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwind-merge": "^2.2.1",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20240314.0",
    "@mdx-js/rollup": "^3.0.1",
    "@remix-run/dev": "^2.8.0",
    "@types/mdx": "^2.0.11",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@typescript-eslint/parser": "^6.7.4",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.18",
    "eslint": "^8.38.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "node-fetch": "^3.3.2",
    "postcss": "^8.4.35",
    "remix-ssg": "^0.0.8",
    "rollup-plugin-visualizer": "^5.12.0",
    "serve": "^14.2.1",
    "tailwindcss": "^3.4.1",
    "typescript": "5.4.2",
    "vite": "^5.1.0",
    "vite-tsconfig-paths": "^4.2.1",
    "wrangler": "^3.24.0"
  },
  "engines": {
    "node": ">=20.0.0",
    "bun": "1.0.31"
  }
}
